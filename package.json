{
  "name": "react-typescript-app-structure",
  "version": "0.1.0",
  "repository": "https://github.com/anlaakso/react-typescript-app-structure",
  "license": "GPL-3.0",
  "scripts": {
    "default": "npm run build:development:once",
    "build:development": "NODE_ENV=\"development\" npm run webpack",
    "build:development:once": "NODE_ENV=\"development\" npm run webpack:once",
    "build:release": "NODE_ENV=\"production\" npm run webpack:uglify:once",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "webpack": "./node_modules/.bin/webpack --watch",
    "webpack:once": "./node_modules/.bin/webpack",
    "webpack:uglify:once": "./node_modules/.bin/webpack -p",
    "lint": "./node_modules/.bin/tslint 'src/**/*.ts' 'src/**/*.tsx'",
    "test": "NODE_ENV=test ./node_modules/.bin/_mocha ./tests --compilers js:babel-core/register,jsx:babel-core/register --require ./tests/setup.js --recursive"
  },
  "devDependencies": {
    "babel-plugin-webpack-alias": "2.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "4.1.2",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.0",
    "husky": "0.14.3",
    "ignore-styles": "5.0.1",
    "jest": "21.2.1",
    "mocha": "3.5.3",
    "nib": "1.1.2",
    "react-css-components": "1.0.1",
    "react-test-renderer": "15.6.1",
    "resolve-url-loader": "2.1.0",
    "sinon": "3.3.0",
    "source-map-loader": "0.2.2",
    "style-loader": "0.18.2",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "ts-jest": "21.1.3",
    "ts-loader": "3.0.5",
    "tslint": "5.8.0",
    "tslint-immutable": "4.4.0",
    "typescript": "2.5.3",
    "webpack": "3.8.1"
  },
  "dependencies": {
    "@types/bem-cn": "2.1.1",
    "@types/enzyme": "3.1.0",
    "@types/history": "4.6.2",
    "@types/jest": "21.1.4",
    "@types/react": "16.0.18",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "5.0.11",
    "@types/react-router": "4.0.17",
    "@types/react-router-dom": "4.2.1",
    "@types/react-router-redux": "^5.0.10",
    "@types/redux": "^3.6.0",
    "@types/redux-saga": "0.10.5",
    "@types/reselect": "2.2.0",
    "bem-cn": "2.1.3",
    "history": "4.7.2",
    "isomorphic-fetch": "2.2.1",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "4.0.8",
    "react-select": "1.0.0-rc.10",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-saga": "0.15.6",
    "reselect": "3.0.1"
  }
}
